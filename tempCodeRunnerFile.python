import matplotlib.pyplot as plt

# Example data representing a distribution
x = [1, 2, 3, 4, 5]
y = [10, 20, 15, 25, 30]

# Create a line plot
plt.plot(x, y, marker='o', linestyle='-')

# Add labels and title
plt.xlabel('X-axis')
plt.ylabel('Y-axis')
plt.title('Line Plot of Distribution')

# Display the plot
plt.grid(True)
plt.show()
